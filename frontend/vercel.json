{
  "framework": "nextjs",
  "installCommand": "npm install",
  "buildCommand": "npm run build:ci",
  "outputDirectory": ".next",
  "crons": [
    {
      "path": "/api/cron/scan",
      "schedule": "0 8 * * *"
    }
  ],
  "functions": {
    "src/app/api/agent/analyze/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/cron/scan/route.ts": {
      "maxDuration": 60
    }
  }
}
